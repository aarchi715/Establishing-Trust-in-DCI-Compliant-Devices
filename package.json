{
  "name": "movie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "server": "python wsgi.py",
    "lint": "next lint",
    "black": "black */*.py",
    "prettier": "prettier --write .",
    "format": "yarn prettier && yarn black",
    "create-env": "pip3 install virtualenv",
    "activate": "source venv/bin/activate",
    "deactivate": "deactivate",
    "install-server": "pip3 install -r requirements.txt",
    "update-server": "pip3 freeze > requirements.txt",
    "venv": "virtualenv venv"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/styles": "^4.11.5",
    "@mui/material": "^5.11.10",
    "axios": "^1.3.3",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "next": "13.1.6",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.4"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "prettier": "^2.8.4"
  }
}
